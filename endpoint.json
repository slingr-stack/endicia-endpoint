{
  "label": "Endicia",
  "name": "endicia",
  "apiVersion": "v1",
  "internal": false,
  "configurationType": "GLOBAL",
  "deployment": {
    "profiles": [
      {
        "name": "default",
        "label": "Default",
        "memory": 256,
        "description": "Default configuration"
      }
    ],
    "allowMultipleInstances": false
  },
  "icon48": "https://storage.googleapis.com/static.slingrs.io/platform/endpoints/endicia.png",
  "configurationHelpUrl": "/endpoints_endicia.html",
  "status": "ACTIVE",
  "stores": [],
  "events": [],
  "functions": [
    {
      "label": "Track by pic number",
      "name": "_trackByPicNumber"
    },
    {
      "label": "Track by pice number",
      "name": "_trackByPieceNumber"
    },
    {
      "label": "Track by transaction id",
      "name": "_trackByTransactionId"
    },
    {
      "label": "Track by reference id",
      "name": "_trackByReferenceId"
    },
    {
      "label": "Listing transactions",
      "name": "_transactionListings"
    }
  ],
  "scripts": [
    "functions.js"
  ],
  "listeners": [],
  "configuration": [
    {
      "name": "accountNumber",
      "label": "Account Number",
      "type": "text",
      "required": true
    },
    {
      "name": "passphrase",
      "label": "Passphrase",
      "type": "text",
      "required": true
    },
    {
      "name": "requesterId",
      "label": "Requester ID",
      "type": "text",
      "required": true
    },
    {
      "label": "API Endpoint",
      "name": "apiEndpoint",
      "type": "buttonsGroup",
      "required": true,
      "defaultValue": "sandbox",
      "typeOptions": {
        "allowCustom": true,
        "possibleValues": [
          {
            "label": "Sandbox",
            "name": "sandbox"
          },
          {
            "label": "Production",
            "name": "production"
          }
        ]
      }
    },
    {
      "name": "productionURL",
      "label": "Production URL",
      "type": "text",
      "required": "config.apiEndpoint == 'production'",
      "visibility": "config.apiEndpoint == 'production'"
    }
  ]
}
